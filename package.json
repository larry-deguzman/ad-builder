{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "scripts": {
    "start": "http-server . -p 8081",
    "combine": "nodemon --exec 'npm run start' server.js",
    "nodemon": "nodemon --exec npm run start",
    "dev": "webpack --mode=development",
    "tsc": "tsc",
    "build:dev": "webpack serve --mode=development --node-env=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch --progress --mode=development",
    "lint": "prettier --write ./src",
    "local": "tsc && npm run dev && npm run combine"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larry-deguzman/building-blocks.git"
  },
  "keywords": [
    "ads",
    "building",
    "blocks",
    "building-blocks",
    "quick",
    "deployment"
  ],
  "author": "Lawrence A de Guzman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larry-deguzman/building-blocks/issues"
  },
  "homepage": "https://github.com/larry-deguzman/building-blocks#readme",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@types/node": "^20.14.2",
    "@webpack-cli/generators": "^3.0.7",
    "babel-loader": "^9.1.3",
    "core-js": "^3.37.1",
    "css-loader": "^7.1.1",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^5.6.0",
    "http-server": "^14.1.1",
    "nodemon": "^3.1.3",
    "path": "^0.12.7",
    "prettier": "^3.3.2",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-strip-block": "^0.3.0"
  },
  "dependencies": {
    "express": "^4.19.2"
  }
}
